{
	"apiVersion": 2,
	"name": "loos/accordion-item",
	"title": "項目",
	"category": "swell-blocks",
	"parent": [
		"loos/accordion"
	],
	"supports": {
		"anchor": true,
		"className": false,
		"reusable": false
	},
	"usesContext": [
		"swell/acc/iconOpened",
		"swell/acc/iconClosed",
		"swell/acc/titleTag"
	],
	"attributes": {
		"title": {
			"type": "string",
			"source": "html",
			"selector": ".swell-block-accordion__label",
			"default": ""
		},
		"iconOpened": {
			"type": "string",
			"default": "icon-caret-up"
		},
		"iconClosed": {
			"type": "string",
			"default": "icon-caret-down"
		},
		"isDefultOpen": {
			"type": "boolean",
			"default": false
		},
		"titleTag": {
			"type": "string",
			"default": "span"
		}
	},
	"textdomain": "swell"
}
